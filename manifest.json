{
  "manifest_version": 2,
  "name": "Inst Downloader",
  "version": "1.0",
  "description": "Расширения, позволяет скачивать фотографии и видео с Instagram",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "chrome"
  ],
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["./extension/entry.js"]
    }
  ],
  "web_accessible_resources": [
    "extension/modules/index.js"
  ]
}